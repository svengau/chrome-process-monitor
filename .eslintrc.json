{
    "extends": "airbnb",
    "parser": "babel-eslint",
    "globals": {
      "__DEV__": true,
      "chrome": true
    },
    "rules": {
      "import/no-extraneous-dependencies": [
        "error", {
        "devDependencies": ["**/*.test.js", "**/*.spec.js", "**/*.stories.js", "webpack.config.js"]}
      ],
      "react/forbid-prop-types": "0",
      "react/jsx-filename-extension": 0
    }
}
