{
  "name": "chrome-process-monitor",
  "private": true,
  "version": "0.0.1",
  "description": "Chrome extension which displays all tabs processes",
  "author": "Sven Gaubert",
  "license": "MIT",
  "scripts": {
    "build": "rm -rf dist && npm run test && NODE_ENV=production webpack -p",
    "build-storybook": "build-storybook",
    "watch": "rm -rf dist && NODE_ENV=development webpack --watch",
    "storybook": "start-storybook -p 6006",
    "package": "cd dist && tar -zcvf chrome-process-monitor.zip .",
    "test": "NODE_ENV=test jest"
  },
  "dependencies": {
    "bytes": "^3.0.0",
    "prop-types": "^15.6.1",
