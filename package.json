    "eslint-plugin-standard": "^3.0.1",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^1.1.10",
    "html-webpack-plugin": "^2.30.1",
    "img-loader": "^2.0.0",
    "jest": "^22.4.2",
    "react-test-renderer": "^16.2.0",
    "storybook-addon-specifications": "^2.1.1",
